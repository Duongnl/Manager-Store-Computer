package 1.Manager-Store-Computer;
import java.util.ArrayList;
import java.util.Scanner;
import do_an.ct_phieuxuat;
public class list_ctpx {
    ArrayList<ct_phieuxuat> list1;
    public list_ctpx()
    {
        this.list1 = new ArrayList<ct_phieuxuat>();
    }
    public list_ctpx(ArrayList<ct_phieuxuat> list)
    {
        this.list1 = list;
    }
    // add
    public void add_pn(ct_phieuxuat pn)
    {
        this.list1.add(pn);
    }
    // output
    public void output()
    {
        for( int i =0 ; i < list1.size(); i++)
        {
            System.out.println(list1.get(i));
        }
    }
    //delete
    public boolean delete(ct_phieuxuat pn)
    {
        return this.list1.remove(pn);
    }
}